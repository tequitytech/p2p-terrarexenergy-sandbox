    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "seed": "ts-node src/scripts/seed-users.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --format json --output-file eslint-report.json",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
