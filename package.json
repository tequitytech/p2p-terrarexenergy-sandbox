  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "rm -rf dist && tsc -p .",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='src/(?!__tests__)'",
